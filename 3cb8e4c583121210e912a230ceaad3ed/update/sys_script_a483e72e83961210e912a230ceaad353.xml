        <script><![CDATA[(function executeRule(current, previous /*null when async*/ ) {
    var gr = new GlideRecord('x_1557802_feedba_1_feedback_data');
    gr.orderByDesc('u_feedback_no');
    gr.query();

    if (gr.next()) {
        gs.info("gr.next()");
        var lastNumber = parseInt(gr.getValue('u_feedback_no'));
        var newNumber = (lastNumber + 1).toString().padStart(8, '0');
        current.setValue('u_feedback_no', newNumber);
        gs.info("--------------newNumber=" + newNumber);
    } else {
        gs.info("gr.next()-else");
        current.setValue('u_feedback_no', '00000001');
    }
})(current, previous);]]></script>
